{
    "manifest_version": 2,
    "name": "volcanos",
    "version": "0.0.1",
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    },
    "background": {"page": "/chrome/chrome.html"},
    "browser_action": {
        "default_icon": "/favicon.png",
        "default_popup": "/chrome/popup.html"
    },
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": ["/proto.js",
                "/lib/base.js",
                "/lib/core.js",
                "/lib/misc.js",
                "/lib/page.js",
                "/lib/user.js",
                "/chrome/contexts.js"]
        }
    ],
    "permissions": [
        "tabs",
        "history",
        "cookies",
        "bookmarks",
        "notifications",
        "contextMenus",
        "*://localhost/*",
        "*://localhost:9020/*"
    ]
}
