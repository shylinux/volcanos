{
    "manifest_version": 2,
    "name": "volcanos",
    "version": "0.0.1",
    "background": {"page": "/publish/chrome/chrome.html"},
    "browser_action": {
        "default_popup": "/publish/chrome/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["/proto.js",
                "/lib/base.js",
                "/lib/core.js",
                "/lib/misc.js",
                "/lib/page.js",
                "/lib/user.js",
                "/publish/chrome/contexts.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "history",
        "cookies",
        "bookmarks",
        "notifications",
        "contextMenus",
        "http://localhost:9020/*"
    ]
}
